<div class="row">
    <div class="col-md-12">
        <div class="table-scroll">
            <table class="table">
                <thead>
                <tr>
                    <th>Question</th>
                    <th>Answer</th>
                    <th>Created by</th>
                    <th>Created at</th>
                    <th>Updated by</th>
                    <th>Updated at</th>
                </tr>
                </thead>
                <tbody>
                <% if (dbView){ %>
                <% for (var i = 0, len = dbView.length; i < len;i++) { %>
                <tr>
                    <td><%= dbView[i].question %></td>
                    <td><%= dbView[i].answer %></td>
                    <td><%= dbView[i].created_by %></td>
                    <td><%= dateParser(dbView[i].created_at).format('DD-MMM-YYYY  HH:mm:ss') %></td>
                    <td><%= dbView[i].updated_by %></td>
                    <td><%= dateParser(dbView[i].updated_at).format('DD-MMM-YYYY  HH:mm:ss') %></td>

                </tr>
                <% }} %>
                </tbody>

            </table>

        </div>
    </div>
</div>