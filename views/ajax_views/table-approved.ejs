<div class="row">
    <div class="col-md-12">
        <div class="table-scroll">
            <table class="table table-hover table-condensed" id="approvedtbl">
                <thead>
                <tr>
                    <th class="hide col-md-1" data-defaultsort="disabled"></th>
                    <th class="col-md-2">Question</th>
                    <th class="col-md-2">Answer</th>
                    <th class="col-md-1">Created by</th>
                    <th class="col-md-2" data-defaultsort="desc">Created at</th>
                    <th class="col-md-1">Updated by</th>
                    <th class="col-md-2">Updated at</th>
                    <th class="col-md-1">Approved by</th>
                    <th class="col-md-1" data-defaultsort="disabled"></th>
                    <!--<th class="col-md-1" data-defaultsort="disabled"></th>-->
                </tr>
                </thead>
                <tbody>
                <% if (dbView){ %>
                <% for (var i = 0, len = dbView.length; i < len;i++) { %>
                <tr>
                    <td class="wordsid hide"><%= dbView[i].entryid %></td>
                    <td><%= dbView[i].question %></td>
                    <td><%= dbView[i].answer %></td>
                    <td><%= dbView[i].created_by %></td>
                    <td><%= dateParser(dbView[i].created_at) %></td>
                    <td><%= dbView[i].updated_by %></td>
                    <td><%= dateParser(dbView[i].updated_at) %></td>
                    <td><%= dbView[i].approved_by %></td>
                    <td>
                        <div class="btn-toolbar pull-right">
                            <button class="btn btn-warning btn-xs declineword"><span
                                        class="glyphicon glyphicon-thumbs-down"></span> decline
                            </button>
                        </div>
                    </td>

                </tr>
                <% }} %>
                </tbody>

            </table>

        </div>
    </div>

</div>